        #self.onStopped() #activate the output of the box
        HeadPitch=self.getParameter("HeadPitch")
        HeadYaw=self.getParameter("HeadYaw")
        rPitch=HeadPitch*math.pi/180
        rYaw=HeadYaw*math.pi/180
        time=self.getParameter("time")
