                                                                            <![CDATA[import math
import numpy
import cv2
import vision_definitions
import time
class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def processing(self,img):
        array = bytearray(img[6])
        img = numpy.array(array)
        img = img.reshape([480, 640, 3])

        bm = numpy.array([0,0,128])#设置阈值
        bx = numpy.array([180,50,255])

        hsv = cv2.cvtColor(img,cv2.COLOR_BGR2HSV)#转换颜色空间
        mask1 = cv2.inRange(hsv,bm,bx)#二值化
        bitwiseGreen = cv2.bitwise_and(img,img,mask=mask1)#对原图和掩模进行位运算
        gray = cv2.cvtColor(bitwiseGreen, cv2.COLOR_BGR2GRAY)#灰度图
        equalizehist = cv2.equalizeHist(gray)#直方图均衡化
        blur = cv2.bilateralFilter(equalizehist, 9, 75, 75)#高斯模糊
        retval, binary = cv2.threshold(blur, 30, 255, cv2.THRESH_BINARY)#30为阈值，blur为传入图像,二值化
        kernel = numpy.ones((5, 5), numpy.uint8)
        final = cv2.morphologyEx(binary,cv2.MORPH_OPEN, kernel)
        return final

    def detectWhite(self,img,p1,p2,p3,p4):
        aMin = 6000
        aSquare = img[p2:p4,p1:p3]
        count = numpy.count_nonzero(aSquare)
        if count>aMin:
            #self.log("detected White Pixel:%d" %count)
            return True
        else:
            return False



    def getCamID(self,str):
        resolution = vision_definitions.kVGA
        colorSpace = vision_definitions.kBGRColorSpace
        fps = 30
        nameId = self.cam.subscribeCamera(str, 1, resolution, colorSpace, fps)
        result = self.cam.setParameter(1,22,2)    #修改相机的内部参数下摄像头开启低光下补光功能
        return nameId

    def onLoad(self):
        #put initialization code here
        #put initialization code here
        self.IP = self.getParameter("IP")
        self.cam = ALProxy("ALVideoDevice",self.IP,9559)#摄像头
        self.motion = ALProxy("ALMotion",self.IP,9559)
        #self.tts = ALProxy("ALTextToSpeech")
        self.bMustStop=False
        self.log("receive")
        pass

    def onUnload(self):
        #put clean-up code here
        self.motion.stopMove()
        pass

    def onInput_onStart(self):
        #self.output_reachB() #activate the output of the box
        self.cam.setActiveCamera(1)
        nameId = self.getCamID(time.strftime('%x'))
        self.bMustStop = False
        left = (0,0,320,240)#设定检测范围，左上角坐标，右下角坐标
        right = (320,0,640,240)
        dleft = (0,240,320,400)
        dright = (320,240,640,400)
        aTurn = 0#记录机器人行进状态

        self.motion.angleInterpolation(["HeadYaw","HeadPitch"],[0.,0.418],[0.5,0.5], True)
        begin2 = time.time()
        while (not self.bMustStop):
            img = self.cam.getImageRemote(nameId)

            final = self.processing(img)

            if (self.detectWhite(final,*left) and self.detectWhite(final,*right)):#到达终点
                self.output_lastdash(aTurn)
                self.output_reachB()
                self.bMustStop = True
            else:
                if (self.detectWhite(final,*left) or self.detectWhite(final,*dleft)):#检测到左侧白线
                    self.output_detected(-1)
                    aTurn = -1
                if (self.detectWhite(final,*right) or self.detectWhite(final,*dright)):#检测到右侧白线
                    self.output_detected(1)
                    aTurn = 1
                if ((not self.detectWhite(final,*left)) and (not self.detectWhite(final,*right)) and
                            (not self.detectWhite(final,*dleft)) and (not self.detectWhite(final,*dright))):#没有检测到线
                    self.output_detected(0)
                    aTurn = 0
                time.sleep(0.1)
            #self.log(time.time())
        end2=time.time()
        self.log("%.2d s" %(end2-begin2))
        self.cam.unsubscribe(nameId)
        self.motion.waitUntilMoveIsFinished()


        pass

    def onInput_onStop(self):
        self.bMustStop = True
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        #self.output_reachB() #activate the output of the box]]>
</content>
